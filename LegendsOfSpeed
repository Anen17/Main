-- Scripts:

local function QLJWY_fake_script() -- Button.LocalScript 
	local script = Instance.new('LocalScript', Button)
	local j = Instance.new('BoolValue', Button)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Value.Value == false then
			script.Parent.Value.Value = true
			_G.StepFarm = true
			while _G.StepFarm == true do 
				local A_1 = "collectOrb"
				local A_2 = "Red Orb"
				local A_3 = "City"
				local Event = game:GetService("ReplicatedStorage").rEvents.orbEvent
				Event:FireServer(A_1, A_2, A_3)
				wait()
			end
		else if script.Parent.Value.Value == true then
				script.Parent.Value.Value = false
				_G.StepFarm = false
				while _G.StepFarm == true do 
					local A_1 = "collectOrb"
					local A_2 = "Red Orb"
					local A_3 = "City"
					local Event = game:GetService("ReplicatedStorage").rEvents.orbEvent
					Event:FireServer(A_1, A_2, A_3)
					wait()
				end
			end
		end
	end)
end
coroutine.wrap(QLJWY_fake_script)()
