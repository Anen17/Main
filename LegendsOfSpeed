local function JMRLURA_fake_script() -- ScriptsHandler.ContainerFunction 
	local script = Instance.new('LocalScript', ScriptsHandler)

	local B = script.Parent.Parent.ContainerContainer
	
	for i,v in pairs(B:GetChildren()) do
		if v:IsA("ScrollingFrame") then
			for i,m in pairs(v:GetChildren()) do
				if m:IsA("Frame") and m.Name == "ToggleFrame1" or m.Name == "ToggleFrame2" or m.Name == "ToggleFrame3" then
					for i,n in pairs(m:GetChildren()) do
						if n:IsA("TextButton") and n.Name == "Button" then
							n.MouseEnter:Connect(function()
								script.Parent.Sound:Play()
							end)
	
							n.MouseButton1Click:Connect(function()
								script.Parent.Sound.Sound:Play()
							end)
							
							n.MouseButton1Click:Connect(function()
								if n.Parent.Frame.BackgroundColor3 == Color3.fromRGB(255, 0, 38) then
									n.Parent.Frame.BackgroundColor3 = Color3.fromRGB(0, 255, 157)
								else
									n.Parent.Frame.BackgroundColor3 = Color3.fromRGB(255, 0, 38)
								end
							end)
						end
					end
				end
			end
		end
	end
	
	local B = script.Parent.Parent.ContainerContainer
	
	for i,v in pairs(B:GetChildren()) do
		if v:IsA("ScrollingFrame") then
			for i,m in pairs(v:GetChildren()) do
				if m:IsA("Frame") and m.Name == "ToggleFrame1" or m.Name == "ToggleFrame2" or m.Name == "ToggleFrame3" or m.Name == "ToggleFrame4" or m.Name == "ToggleFrame5" or m.Name == "ToggleFrame6" or m.Name == "ToggleFrame7" or m.Name == "ToggleFrame8" then
					m.MouseEnter:Connect(function()
						local info = TweenInfo.new(0.25)
	
						local tween = game:GetService("TweenService"):Create(m,info,{BackgroundColor3 = Color3.fromRGB(55, 55, 55)})
						tween:Play()
					end)
	
					m.MouseLeave:Connect(function()
						local info = TweenInfo.new(0.25)
	
						local tween = game:GetService("TweenService"):Create(m,info,{BackgroundColor3 = Color3.fromRGB(25, 25, 25)})
						tween:Play()
					end)
				end
			end
		end
	end
	
	local B = script.Parent.Parent.ContainerContainer
	
	for i,v in pairs(B:GetChildren()) do
		if v:IsA("ScrollingFrame") then
			for i,m in pairs(v:GetChildren()) do
				if m:IsA("Frame") and m.Name == "ToggleFrame1" or m.Name == "ToggleFrame2" or m.Name == "ToggleFrame3" or m.Name == "ToggleFrame4" or m.Name == "ToggleFrame5" or m.Name == "ToggleFrame6" or m.Name == "ToggleFrame7" or m.Name == "ToggleFrame8" then
					for i,n in pairs(m:GetChildren()) do
						if n:IsA("TextButton") and n.Name == "Button" then
							local ms = game.Players.LocalPlayer:GetMouse()
	
							local btn = n
							local sample = script.Parent:WaitForChild('Sample')
	
							btn.MouseButton1Click:Connect(function()
								local c = sample:Clone()
								c.Parent = btn
								local x, y = (ms.X - c.AbsolutePosition.X), (ms.Y - c.AbsolutePosition.Y)
								c.Position = UDim2.new(0, x, 0, y)
								local len, size = 0.35, nil
								if btn.AbsoluteSize.X >= btn.AbsoluteSize.Y then
									size = (btn.AbsoluteSize.X * 1.5)
								else
									size = (btn.AbsoluteSize.Y * 1.5)
								end
								c:TweenSizeAndPosition(UDim2.new(0, size, 0, size), UDim2.new(0.5, (-size / 2), 0.5, (-size / 2)), 'Out', 'Quad', len, true, nil)
								for i = 1, 10 do
									c.ImageTransparency = c.ImageTransparency + 0.05
									wait(len / 12)
								end
								c:Destroy()
							end)
						end
					end
				end
			end
		end
	end
end
coroutine.wrap(JMRLURA_fake_script)()
local function RJEZT_fake_script() -- ScriptsHandler.OtherFunction 
	local script = Instance.new('LocalScript', ScriptsHandler)

	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
		dragToggle = nil
		local dragSpeed = 1
		dragInput = nil
		dragStart = nil
		local dragPos = nil
		function updateInput(input)
			local Delta = input.Position - dragStart
			local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
			game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.25), {Position = Position}):Play()
		end
		Frame.InputBegan:Connect(function(input)
			if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
				dragToggle = true
				dragStart = input.Position
				startPos = Frame.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragToggle = false
					end
				end)
			end
		end)
		Frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if input == dragInput and dragToggle then
				updateInput(input)
			end
		end)
	end
	
	dragify(script.Parent.Parent)
	
	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	
	-- Fetch the thumbnail
	local userId = player.UserId
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size420x420
	local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	
	-- Set the ImageLabel's content to the user thumbnail
	local imageLabel = script.Parent.Parent.Frame.NameFrame.UserImage
	imageLabel.Image = content
	
	while wait(game:IsLoaded()) do
		local text = game.Players.LocalPlayer.Name
	
		for i = 1, #text do
			script.Parent.Parent.Frame.NameFrame.TextLabel.Text = string.sub(text, 1, i)
			wait(0.2) 
		end
		break
	end
end
coroutine.wrap(RJEZT_fake_script)()
